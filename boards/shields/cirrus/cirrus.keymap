/ {
        keymap {
                compatible = "zmk,keymap";

                default_layer {
                        display-name = "Default Layer";



                        bindings = <
   &trans       &kp K_POWER  &kp ESC      &kp N1   &kp N2 &kp N3  &kp N4    &kp N5                       &kp N6     &kp N7      &kp N8    &kp N9    &kp N0     &kp BSPC    &kp DEL     &trans            
